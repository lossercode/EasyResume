{
  "name": "EasyResume",
  "description": "AI简历填写助手",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "tabs",
    "debugger",
    "scripting"
  ],
  "incognito": "split",
  "background": {
    "service_worker": "./scripts/worker.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": "logo_48.png",
    "default_title": "EasyResume",
    "default_popup": "./index.html"
  }
}